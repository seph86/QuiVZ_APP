{
  "name": "app2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart.js": "^3.6.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "cra-append-sw": "^2.7.0",
    "dompurify": "^2.3.3",
    "http-server": "^13.0.2",
    "inferno": "^7.4.10",
    "inferno-extras": "^7.4.10",
    "inferno-scripts": "7.0.4"
  },
  "scripts": {
    "start": "inferno-scripts start",
    "build": "inferno-scripts build && cat src/custom-service-worker.js >> build/service-worker.js",
    "serve-old": "http-server -S -C ./cert.pem -K ./key.pem -p 8000 --cors https://192.168.2.2:8080 ./build",
    "serve": "http-server --cors http://localhost:8080 -p 8000 ./build",
    "test": "inferno-scripts test",
    "eject": "inferno-scripts eject"
  },
  "eslintConfig": {
    "extends": "inferno-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
